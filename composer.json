{
  "name": "rollun-com/rollun-promise",
  "description": "Promise",
  "license": "GPL-3.0",
  "type": "library",
  "authors": [
    {
      "name": "avz-cmf",
      "email": "email@example.com"
    },
    {
      "name": "victorynox",
      "email": "it.proffesor02@gmail.com"
    }
  ],
  "require": {
    "php": "^5.6 || ^7.0",
    "roave/security-advisories": "dev-master",
    "zendframework/zend-expressive": "^1.0",
    "zendframework/zend-expressive-helpers": "^2.0",
    "zendframework/zend-expressive-fastroute": "^1.0",
    "zendframework/zend-servicemanager": "^3.0",
    "zendframework/zend-stdlib": "^3",
    "zendframework/zend-db": "2.8.2",
    "opis/closure": "^2.1.0",
    "rollun-com/rollun-installer": "^2.0",
    "rollun-com/rollun-datastore": "^3.0",
    "rollun-com/rollun-utils": "^2.0",
    "rollun-com/rollun-dic": "^1.0",
    "rollun-com/rollun-logger": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.0",
    "squizlabs/php_codesniffer": "^2.3",
    "filp/whoops": "^1.1 || ^2.0"
  },
  "autoload": {
    "psr-4": {
      "rollun\\promise\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "rollun\\test\\promise\\": "tests/src"
    }
